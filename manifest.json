{
  "manifest_version": 3,
  "name": "Meet-Detector",
  "version": "1.0",
  "options_page": "settings.html",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://meet.google.com/*"
  ]
}
